{
  "name": "ts-monoid",
  "version": "1.0.0",
  "description": "Aplicar o estudo de monoids, utilizando Typescript e Ramda",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": " node_modules/.bin/nodemon --exec 'yarn run lint && node_modules/.bin/ts-node src/index.ts'",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts' -t verbose --force",
    "format": "prettier --write \"src/**/*.{ts}\""
  },
  "dependencies": {
    "@types/node": "^12.12.14",
    "nodemon": "^2.0.1",
    "ramda": "^0.26.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/ramda": "types/npm-ramda#dist",
    "prettier": "1.19.1"
  }
}
